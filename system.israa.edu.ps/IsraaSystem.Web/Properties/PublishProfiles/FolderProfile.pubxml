<?xml version="1.0" encoding="utf-8"?>
<!--
This file is used by the publish/package process of your Web project. You can customize the behavior of this process
by editing this MSBuild file. In order to learn more about this please visit https://go.microsoft.com/fwlink/?LinkID=208121. 
-->
<Project ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup>
    <WebPublishMethod>MSDeploy</WebPublishMethod>
    <LastUsedBuildConfiguration>Release</LastUsedBuildConfiguration>
    <LastUsedPlatform>Any CPU</LastUsedPlatform>
    <SiteUrlToLaunchAfterPublish>http://system.israa.edu.ps:80/</SiteUrlToLaunchAfterPublish>
    <LaunchSiteAfterPublish>True</LaunchSiteAfterPublish>
    <ExcludeApp_Data>False</ExcludeApp_Data>
    <MSDeployServiceURL>https://israa.edu.ps:8172/msdeploy.axd</MSDeployServiceURL>
    <DeployIisAppPath>system.israa.edu.ps</DeployIisAppPath>
    <RemoteSitePhysicalPath />
    <SkipExtraFilesOnServer>True</SkipExtraFilesOnServer>
    <MSDeployPublishMethod>WMSVC</MSDeployPublishMethod>
    <EnableMSDeployBackup>False</EnableMSDeployBackup>
    <UserName>ISRAA\UserWebDeploy</UserName>
    <_SavePWD>True</_SavePWD>
    <PublishDatabaseSettings>
      <Objects xmlns="">
        <ObjectGroup Name="IsraPermissionEntities" Order="1" Enabled="False">
          <Destination Path="" />
          <Object Type="DbCodeFirst">
            <Source Path="DBContext" DbContext="IsraaSystem.EntityFrameWork.IsraaPermission.IsraPermissionEntities, IsraaSystem.EntityFrameWork" Origin="Configuration" />
          </Object>
        </ObjectGroup>
        <ObjectGroup Name="IsraaHumanResourceEntities" Order="2" Enabled="False">
          <Destination Path="" />
          <Object Type="DbCodeFirst">
            <Source Path="DBContext" DbContext="IsraaSystem.DataAccess.EntityFrameWork.IsraaHumanResource.IsraaHumanResourceEntities, IsraaSystem.EntityFrameWork" Origin="Configuration" />
          </Object>
        </ObjectGroup>
        <ObjectGroup Name="IsraaAcademicEntities" Order="3" Enabled="False">
          <Destination Path="" />
          <Object Type="DbCodeFirst">
            <Source Path="DBContext" DbContext="IsraaSystem.EntityFrameWork.IsraaAcademic.IsraaAcademicEntities, IsraaSystem.EntityFrameWork" Origin="Configuration" />
          </Object>
        </ObjectGroup>
        <ObjectGroup Name="IsraaSystem.EntityFrameWork.Common.IsraaDbContext" Order="4" Enabled="False">
          <Destination Path="" />
          <Object Type="DbCodeFirst">
            <Source Path="DBContext" DbContext="IsraaSystem.EntityFrameWork.Common.IsraaDbContext, IsraaSystem.EntityFrameWork" Origin="Convention" />
          </Object>
        </ObjectGroup>
        <ObjectGroup Name="Hangfire" Order="5" Enabled="False">
          <Destination Path="" />
          <Object Type="DbDacFx">
            <PreSource Path="Data Source=.;Initial Catalog=Hangfire;Integrated Security=True;" includeData="False" />
            <Source Path="$(IntermediateOutputPath)AutoScripts\Hangfire_IncrementalSchemaOnly.dacpac" dacpacAction="Deploy" />
          </Object>
          <UpdateFrom Type="Web.Config">
            <Source MatchValue="Data Source=.;Initial Catalog=Hangfire;Integrated Security=True;" MatchAttributes="$(UpdateFromConnectionStringAttributes)" />
          </UpdateFrom>
        </ObjectGroup>
      </Objects>
    </PublishDatabaseSettings>
  </PropertyGroup>
  <ItemGroup>
    <MSDeployParameterValue Include="$(DeployParameterPrefix)Hangfire-Web.config Connection String" />
    <MSDeployParameterValue Include="$(DeployParameterPrefix)IsraaAcademicEntities-Web.config Connection String" />
    <MSDeployParameterValue Include="$(DeployParameterPrefix)IsraaHumanResourceEntities-Web.config Connection String" />
    <MSDeployParameterValue Include="$(DeployParameterPrefix)IsraaSystem.EntityFrameWork.Common.IsraaDbContext-Web.config Connection String" />
    <MSDeployParameterValue Include="$(DeployParameterPrefix)IsraPermissionEntities-Web.config Connection String" />
  </ItemGroup>
  <ItemGroup>
    <_ConnectionStringsToInsert Include="IsraaSystem.EntityFrameWork.Common.IsraaDbContext" />
  </ItemGroup>
</Project>