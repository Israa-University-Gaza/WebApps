@using System.Data
@model DataTable
@{
    ViewBag.Title = "شؤون الموظفين - أنواع الإجازات";
    ViewBag.Menu = "#menu-holiday,#menu-holiday-type";
    Layout = "~/Areas/HumanResource/Views/Shared/_Layout.cshtml";
}


@section PageBreadcrumb{
    <li>
        <a>الرئيسية</a>
        <i class="fa fa-circle"></i>
    </li>
    <li>
        <a>العطل الرسمية</a>
        <i class="fa fa-circle"></i>
    </li>
    <li>
        <a>أنواع العطل الرسمية</a>
    </li>
}

<div class="row">
    <div class="col-md-12">
        <div class="portlet light bordered">
            <div class="portlet-title">
                <div class="caption">
                    <i class="icon-social-dribbble font-green"></i>
                    <span class="caption-subject font-green bold uppercase">أنواع العطل الرسمية</span>
                </div>
                <div class="actions">
                    <a href="/HumanResource/HolidayType/Add" class="btn btn-primary pop-up-md">إضافة نوع عطلة رسمية</a>
                </div>
            </div>
            <div class="portlet-body">
                @Html.Partial("List", Model)
            </div>
        </div>
    </div>
</div>


