}
<div class="row">
    <div class="col-md-12">
        <div class="portlet light bordered">
            <div class="portlet-title">
                <div class="caption">
                    <i class="icon-social-dribbble font-green"></i>
                    <span class="caption-subject font-green bold uppercase">عرض احصائية دوام المحاضرين</span>
                </div>
            </div>
            <div class="portlet-body">
                @Html.Partial("StatisticsList", Model)
