    ViewBag.Menu = "#menu-attendance-instructor-statistic";
    Layout = "~/Areas/HumanResource/Views/Shared/_Layout.cshtml";
}

@section PageBreadcrumb{
    <li>
        <a href="/HumanResource/Dashboard">الرئيسية</a>
        <i class="fa fa-circle"></i>
    </li>
    <li>
        <a href="/HumanResource/ContractInstructor/StatisticIndex">احصائية دوام المحاضرين</a>
        <i class="fa fa-circle"></i>
    </li>
    <li>
        <a>عرض الإحصائيات</a>
